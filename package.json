{
  "name": "@coderscamp2020/project-typescript-chess",
  "version": "1.0.0",
  "description": ".CodersChess - 2nd group project @CodersCamp2020 by Fron znad zatoki",
  "main": "index.ts",
  "scripts": {
    "build": "rimraf dist && parcel build index.html --public-url ./",
    "clean": "rimraf .cache && rimraf coverage && rimraf dist && rimraf node_modules",
    "format": "prettier --config .prettierrc --write \"src/**/*.ts\" \"styles/**/*.scss\" \"test/**/*.ts\" \"**/*.json\"",
    "lint": "eslint . --ext .ts",
    "start:dev": "parcel -p 8765 watch index.html",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "chess",
    "CodersCamp",
    "FrontZnadZatoki"
  ],
  "author": "Front znad zatoki @CodersCamp2020",
  "license": "ISC",
  "homepage": "",
  "bugs": {
    "url": ""
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@testing-library/dom": "^7.29.4",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/user-event": "^12.6.0",
    "@types/node": "^14.14.21",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-no-loops": "^0.3.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "normalize.css": "^8.0.1",
    "regenerator-runtime": "^0.13.7"
  },
  "jest": {
    "testRegex": "(spec|test)[.]ts",
    "coverageDirectory": "coverage",
    "setupFiles": [
      "./test/setupJest.ts"
    ],
    "collectCoverageFrom": [
      "./src/**"
    ]
  }
}
